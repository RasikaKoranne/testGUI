wd_message,wd_db_msg_desc 
from ws_wrk_error_log
order by nvl(wd_row_number,0),wd_time_stamp, wd_job, wd_task;


_TABLE_ ws_admin_v_sched
create or replace view ws_admin_v_sched as
select  
	'Waiting' "Type",
	wjc_name "Job Name",
	decode(wjc_status,'H','On Hold','R','Running','P','Pending','W','Waiting','C','Completed',
			'B','Blocked','F','Failed','G','Failed - Aborted','E','Error Completion','Unknown') "Status",
	wjc_sequence "Sequence", 
	wjc_start_after "Started/Scheduled",
	sysdate "Completed", 
	0 "Hours Elapsed" ,
	0 "Minutes Elapsed", 
	0 "Okay",
	0 "Info",
	0 "Warn",
	0 "Detail",
	0 "Error"
	  from ws_wrk_job_ctrl
union  
	 select 'Running',wjr_name,
	decode(wjr_status,'H','On Hold','R','Running','P','Pending','W','Waiting','C','Completed',
			'B','Blocked','F','Failed','G','Failed - Aborted','E','Error Completion','Unknown') "Status",
	wjr_sequence, wjr_started, wjr_completed, 
	 trunc(to_number(sysdate-wjr_started)*24,0),  
	 round(to_number(sysdate-wjr_started)*24*60,0)  
	 - (trunc(to_number(sysdate-wjr_started)*24,0)*60),  
	 wjr_okay_count, 
	 wjr_info_count, wjr_warning_count, wjr_detail_count, 
	 wjr_error_count
	  from ws_wrk_job_run 
union  
	 select 'Finished',wjl_name,
	decode(wjl_status,'H','On Hold','R','Running','P','Pending','W','Waiting','C','Completed',
			'B','Blocked','F','Failed','G','Failed - Aborted','E','Error Completion','Unknown') "Status",
	 wjl_sequence, wjl_started, wjl_completed, 
	 wjl_elapsed_hh, wjl_elapsed_mi, wjl_okay_count, 
	 wjl_info_count, wjl_warning_count, wjl_detail_count, 
	 wjl_error_count
	 from ws_wrk_job_log  
order by 1 DESC,4 DESC  ;


_TABLE_ ws_admin_v_task
create or replace view ws_admin_v_task as
select  
	'Running' "result_msg",wtr_name,
	decode(wtr_run_status,'H','On Hold','R','Running','P','Pending','W','Waiting','C','Completed',
			'B','Blocked','F','Failed','G','Failed - Aborted','E','Error Completion','Unknown') "status",
	wtr_sequence, wtr_started, wtr_completed, 
	 trunc(to_number(sysdate-wtr_started)*24,0) "elapsed_HH",  
	 round(to_number(sysdate-wtr_started)*24*60,0)  
	 - (trunc(to_number(sysdate-wtr_started)*24,0)*60) "elapsed_MI",  
	  wtr_info_count, wtr_warning_count, wtr_detail_count, 
	 wtr_rec_inserted, wtr_rec_updated,wtr_rec_replaced,wtr_rec_deleted,wtr_rec_discarded,
	wtr_rec_rejected,wtr_rec_errored
	  from ws_wrk_task_run 
union  
	 select wtl_return_msg,wtl_name,
	decode(wtl_run_status,'H','On Hold','R','Running','P','Pending','W','Waiting','C','Completed',
			'B','Blocked','F','Failed','G','Failed - Aborted','E','Error Completion','Unknown') "Status",
	 wtl_sequence, wtl_started, wtl_completed, 
	 wtl_elapsed_hh, wtl_elapsed_mi, 

	 wtl_info_count, wtl_warning_count, wtl_detail_count, 
	 wtl_rec_inserted, wtl_rec_updated,wtl_rec_replaced,wtl_rec_deleted,wtl_rec_discarded,
	wtl_rec_rejected,wtl_rec_errored
	 from ws_wrk_task_log;


