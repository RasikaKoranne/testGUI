_TABLE_ ws_rep_v_track_back
CREATE OR REPLACE VIEW ws_rep_v_track_back
AS
SELECT lt_table_name rb_table_name
     , DECODE(lc_src_table,lt_table_name,NULL,lc_src_table) rb_src_table
     , DECODE(SUBSTR(DC_ATTRIBUTES,INSTR(DC_ATTRIBUTES,'DataWarehouse;'),14),'DataWarehouse;','N','Y') rb_remote_flag
FROM   ws_load_tab
     , ws_load_col
     , ws_dbc_connect
WHERE  lt_obj_key = lc_obj_key
AND    lt_connect_key = dc_obj_key
AND    lt_table_name <> lc_src_table
UNION
SELECT st_table_name rb_table_name
     , DECODE(sc_src_table,st_table_name,NULL,sc_src_table) rb_src_table
     , 'N' rb_remote_flag
FROM   ws_stage_tab
     , ws_stage_col
WHERE  st_obj_key = sc_obj_key
AND    NVL(sc_join_flag,'N') <> 'Y'
AND    st_table_name <> sc_src_table
UNION
SELECT dt_table_name rb_table_name
     , DECODE(dc_src_table,dt_table_name,NULL,dc_src_table) rb_src_table
     , 'N' rb_remote_flag
FROM   ws_dim_tab
     , ws_dim_col
WHERE  dt_obj_key = dc_obj_key
AND    dt_table_name <> dc_src_table
UNION
SELECT ft_table_name rb_table_name
     , DECODE(fc_src_table,ft_table_name,NULL,fc_src_table) rb_src_table
     , 'N' rb_remote_flag
FROM   ws_fact_tab
     , ws_fact_col
WHERE  ft_obj_key = fc_obj_key
AND    NVL(fc_join_flag,'N') <> 'Y'
AND    ft_table_name <> fc_src_table
UNION
SELECT at_table_name rb_table_name
     , DECODE(ac_src_table,at_table_name,NULL,ac_src_table) rb_src_table
     , 'N' rb_remote_flag
FROM   ws_agg_tab
     , ws_agg_col
WHERE  at_obj_key = ac_obj_key
AND    NVL(ac_join_flag,'N') <> 'Y'
AND    at_table_name <> ac_src_table
UNION
SELECT ct_cube_name rb_table_name
     , ct_fact_table AS rb_src_table
     , 'N' AS rb_remote_flag
FROM   ws_cube_tab
;

