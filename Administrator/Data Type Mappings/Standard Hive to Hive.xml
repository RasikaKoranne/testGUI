<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<data_type_mapping_set name="Standard Hive to Hive" xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd">

  <mapping_set_description>Standard Hive to Hive data type mapping.
Please do not edit this standard mapping file as any changes will be overwritten on upgrades.
See the user guide for information on how to create custom mapping files</mapping_set_description>

  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>

  <data_type_mapping match_data_type="tinyint" output_data_type="tinyint">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="smallint" output_data_type="smallint">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="int" output_data_type="int">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="bigint" output_data_type="bigint">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="boolean" output_data_type="boolean">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="float" output_data_type="float">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="double" output_data_type="double">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="string" output_data_type="string">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="binary" output_data_type="binary">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="sql_type_timestamp" output_data_type="timestamp">
    <match_parameters>
      <match_length>*</match_length>
      <match_precision>*</match_precision>
      <match_scale>*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal">
    <match_parameters>
      <match_precision>38</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision,%scale)"></data_type_mapping>

  <data_type_mapping match_data_type="date" output_data_type="date"></data_type_mapping>

  <data_type_mapping match_data_type="char" output_data_type="char(%length)"></data_type_mapping>

  <data_type_mapping match_data_type="varchar" output_data_type="varchar(%length)"></data_type_mapping>

</data_type_mapping_set>
