<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<data_type_mapping_set name="Standard Other to ANSI" xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd">

  <mapping_set_description>Standard Other to ANSI data type mapping.
Please do not edit this standard mapping file as any changes will be overwritten on upgrades.
See the user guide for information on how to create custom mapping files.</mapping_set_description>

  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>

  <data_type_mapping match_data_type="float" output_data_type="float"/>

  <data_type_mapping match_data_type="double precision" output_data_type="double precision"/>

  <data_type_mapping match_data_type="double" output_data_type="double precision"/>

  <data_type_mapping match_data_type="number" output_data_type="numeric(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="number" output_data_type="numeric(%precision,%scale)"/>

  <data_type_mapping match_data_type="smallint" output_data_type="smallint"/>

  <data_type_mapping match_data_type="int" output_data_type="int"/>

  <data_type_mapping match_data_type="bigint" output_data_type="bigint"/>

  <data_type_mapping match_data_type="bit" output_data_type="bit"/>

  <data_type_mapping match_data_type="byteint" output_data_type="integer"/>

  <data_type_mapping match_data_type="real" output_data_type="real"/>

  <data_type_mapping match_data_type="counter" output_data_type="integer"/>

  <data_type_mapping match_data_type="integer" output_data_type="integer"/>

  <data_type_mapping match_data_type="rowid" output_data_type="integer"/>

  <data_type_mapping match_data_type="time" output_data_type="time"/>

  <data_type_mapping match_data_type="date" output_data_type="date"/>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp"/>

  <data_type_mapping match_data_type="datetime" output_data_type="date"/>

  <data_type_mapping match_data_type="smalldatetime" output_data_type="datetime"/>

  <data_type_mapping match_data_type="currency" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="currency" output_data_type="decimal(%precision,%scale)"/>

  <data_type_mapping match_data_type="money" output_data_type="decimal(%precision,%scale)"/>

  <data_type_mapping match_data_type="smallmoney" output_data_type="decimal(%precision,%scale)"/>

  <data_type_mapping match_data_type="char" output_data_type="char(%precision)"/>

  <data_type_mapping match_data_type="varchar2" output_data_type="varchar(%precision)"/>

  <data_type_mapping match_data_type="nvarchar" output_data_type="varchar(max)">
    <match_parameters>
      <match_precision>0</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="nvarchar" output_data_type="varchar(%precision)"/>

  <data_type_mapping match_data_type="nchar" output_data_type="nchar(%precision)"/>

  <data_type_mapping match_data_type="varchar" output_data_type="varchar(max)">
    <match_parameters>
      <match_precision>0</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="varchar" output_data_type="varchar(%precision)"/>

  <data_type_mapping match_data_type="uniqueidentifier" output_data_type="integer"/>

</data_type_mapping_set>
