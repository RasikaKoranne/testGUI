<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<data_type_mapping_set name="Standard PDW to PDW" xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd">

  <mapping_set_description>Standard PDW to PDW data type mapping.
Please do not edit this standard mapping file as any changes will be overwritten on upgrades.
See the user guide for information on how to create custom mapping files.</mapping_set_description>

  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>

  <data_type_mapping match_data_type="bigint" output_data_type="bigint"></data_type_mapping>
  
  <data_type_mapping match_data_type="binary" output_data_type="binary">
    <match_parameters>
			<match_precision>1</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="binary" output_data_type="binary(%precision)"></data_type_mapping>

  <data_type_mapping match_data_type="bit" output_data_type="bit"></data_type_mapping>

  <data_type_mapping match_data_type="char" output_data_type="char">
    <match_parameters>
      <match_length>1</match_length>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="char" output_data_type="char(%precision)"></data_type_mapping>

  <data_type_mapping match_data_type="date" output_data_type="date"></data_type_mapping>

  <data_type_mapping match_data_type="datetime" output_data_type="datetime"></data_type_mapping>

  <data_type_mapping match_data_type="datetime2" output_data_type="datetime2(%scale)"></data_type_mapping>

  <data_type_mapping match_data_type="datetimeoffset" output_data_type="datetimeoffset">
    <match_parameters>
      <match_scale>7</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="datetimeoffset" output_data_type="datetimeoffset(%scale)"></data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision,%scale)"></data_type_mapping>

  <data_type_mapping match_data_type="float" output_data_type="float"></data_type_mapping>

  <data_type_mapping match_data_type="real" output_data_type="real"></data_type_mapping>

  <data_type_mapping match_data_type="int" output_data_type="int"></data_type_mapping>

  <data_type_mapping match_data_type="money" output_data_type="money"></data_type_mapping>

  <data_type_mapping match_data_type="nchar" output_data_type="nchar(%precision)"></data_type_mapping>

  <data_type_mapping match_data_type="nvarchar" output_data_type="nvarchar(%precision)"></data_type_mapping>

  <data_type_mapping match_data_type="smalldatetime" output_data_type="smalldatetime"></data_type_mapping>

  <data_type_mapping match_data_type="smallint" output_data_type="smallint"></data_type_mapping>

  <data_type_mapping match_data_type="smallmoney" output_data_type="smallmoney"></data_type_mapping>

  <data_type_mapping match_data_type="time" output_data_type="time(%scale)"></data_type_mapping>

  <data_type_mapping match_data_type="tinyint" output_data_type="tinyint"></data_type_mapping>

  <data_type_mapping match_data_type="varbinary" output_data_type="varbinary(%precision)"></data_type_mapping>

  <data_type_mapping match_data_type="varchar" output_data_type="varchar(%length)"></data_type_mapping>

</data_type_mapping_set>
