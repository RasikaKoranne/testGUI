<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<data_type_mapping_set name="Standard Oracle to Oracle" xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd">

  <mapping_set_description>Standard Oracle to Oracle data type mapping.
Please do not edit this standard mapping file as any changes will be overwritten on upgrades.
See the user guide for information on how to create custom mapping files.</mapping_set_description>

  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>

  <data_type_mapping match_data_type="number" output_data_type="number">
    <match_parameters>
      <match_precision>39-*</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="number" output_data_type="number">
    <match_parameters>
      <match_precision>15</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="number" output_data_type="number(max)">
    <match_parameters>
      <match_precision>0</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="number" output_data_type="number(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="number" output_data_type="number(%precision,%scale)"/>

  <data_type_mapping match_data_type="binary_float" output_data_type="binary_float"/>

  <data_type_mapping match_data_type="binary_double" output_data_type="binary_double"/>

  <data_type_mapping match_data_type="float" output_data_type="float"/>

  <data_type_mapping match_data_type="date" output_data_type="date"/>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp">
    <match_parameters>
      <match_scale>6</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp(%scale)">
    <match_parameters>
      <match_scale>*-5</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp(%scale)">
    <match_parameters>
      <match_scale>7-*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with time zone" output_data_type="timestamp with time zone">
    <match_parameters>
      <match_scale>6</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with time zone" output_data_type="timestamp(%scale) with time zone">
    <match_parameters>
      <match_scale>*-5</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with time zone" output_data_type="timestamp(%scale) with time zone">
    <match_parameters>
      <match_scale>7-*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with local timezone" output_data_type="timestamp with local time zone">
    <match_parameters>
      <match_scale>6</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with local timezone" output_data_type="timestamp(%scale) with local time zone">
    <match_parameters>
      <match_scale>*-5</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with local timezone" output_data_type="timestamp(%scale) with local time zone">
    <match_parameters>
      <match_scale>7-*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with local time z" output_data_type="timestamp with local time zone">
    <match_parameters>
      <match_scale>6</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with local time z" output_data_type="timestamp(%scale) with local time zone">
    <match_parameters>
      <match_scale>*-5</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="timestamp with local time z" output_data_type="timestamp(%scale) with local time zone">
    <match_parameters>
      <match_scale>7-*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="interval year to month" output_data_type="interval year to month"/>

  <data_type_mapping match_data_type="interval day to second" output_data_type="interval day to second"/>

  <data_type_mapping match_data_type="varchar" output_data_type="varchar2(%precision)"/>

  <data_type_mapping match_data_type="long" output_data_type="long"/>

  <data_type_mapping match_data_type="blob" output_data_type="blob"/>

  <data_type_mapping match_data_type="clob" output_data_type="clob"/>

  <data_type_mapping match_data_type="nclob" output_data_type="nclob"/>

  <data_type_mapping match_data_type="bfile" output_data_type="bfile"/>

  <data_type_mapping match_data_type="rowid" output_data_type="rowid"/>

  <data_type_mapping match_data_type="urowid" output_data_type="urowid"/>

</data_type_mapping_set>
