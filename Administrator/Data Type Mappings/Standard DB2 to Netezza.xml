<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<data_type_mapping_set name="Standard DB2 to Netezza" xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd">

  <mapping_set_description>Standard DB2 to Netezza data type mapping.</mapping_set_description>

  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>

  <data_type_mapping match_data_type="smallint" output_data_type="smallint"/>

  <data_type_mapping match_data_type="integer" output_data_type="integer"/>

  <data_type_mapping match_data_type="bigint" output_data_type="bigint"/>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision,%scale)"/>

  <data_type_mapping match_data_type="numeric" output_data_type="numeric(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="numeric" output_data_type="numeric(%precision,%scale)"/>

  <data_type_mapping match_data_type="real" output_data_type="real"/>

  <data_type_mapping match_data_type="double" output_data_type="double"/>

  <data_type_mapping match_data_type="decfloat" output_data_type="float"/>

  <data_type_mapping match_data_type="float" output_data_type="float">
    <match_parameters>
      <match_precision>0</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="float" output_data_type="float(%precision)">
    <match_parameters>
      <match_precision>1-*</match_precision>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="char" output_data_type="char(%precision)"/>

  <data_type_mapping match_data_type="varchar" output_data_type="varchar(%precision)"/>

  <data_type_mapping match_data_type="long varchar" output_data_type="varchar(32700)"/>

  <data_type_mapping match_data_type="date" output_data_type="date"/>

  <data_type_mapping match_data_type="time" output_data_type="varchar(8)"/>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp"/>

</data_type_mapping_set>
