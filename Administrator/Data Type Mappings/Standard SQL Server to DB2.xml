<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<data_type_mapping_set name="Standard SQL Server to DB2" xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd">

  <mapping_set_description>Standard SQL Server to DB2 data type mapping.
Please do not edit this standard mapping file as any changes will be overwritten on upgrades.
See the user guide for information on how to create custom mapping files.</mapping_set_description>

  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>

  <data_type_mapping match_data_type="numeric" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_precision>*-31</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="numeric" output_data_type="decimal(31)">
    <match_parameters>
      <match_precision>32-*</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="numeric" output_data_type="decimal(31,31)">
    <match_parameters>
      <match_scale>32-*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="numeric" output_data_type="decimal(31,%scale)">
    <match_parameters>
      <match_precision>32-*</match_precision>
      <match_scale>*-31</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="numeric" output_data_type="decimal(%precision,%scale)"/>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_precision>*-31</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(31)">
    <match_parameters>
      <match_precision>32-*</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(31,31)">
    <match_parameters>
      <match_scale>32-*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(31,%scale)">
    <match_parameters>
      <match_precision>32-*</match_precision>
      <match_scale>*-31</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision,%scale)"/>

  <data_type_mapping match_data_type="smallmoney" output_data_type="decimal(10,4)"/>

  <data_type_mapping match_data_type="money" output_data_type="decimal(19,4)"/>

  <data_type_mapping match_data_type="bit" output_data_type="smallint"/>

  <data_type_mapping match_data_type="int" output_data_type="integer"/>
  
  <data_type_mapping match_data_type="int identity" output_data_type="integer"/>

  <data_type_mapping match_data_type="tinyint" output_data_type="smallint"/>

  <data_type_mapping match_data_type="tinyint identity" output_data_type="smallint"/>

  <data_type_mapping match_data_type="bigint" output_data_type="bigint"/>

  <data_type_mapping match_data_type="bigint identity" output_data_type="bigint"/>

  <data_type_mapping match_data_type="smallint" output_data_type="smallint"/>

  <data_type_mapping match_data_type="smallint identity" output_data_type="smallint"/>

  <data_type_mapping match_data_type="float" output_data_type="double"/>

  <data_type_mapping match_data_type="real" output_data_type="double"/>

  <data_type_mapping match_data_type="char" output_data_type="char(%length)">
    <match_parameters>
      <match_length>*-254</match_length>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="char" output_data_type="char(254)">
    <match_parameters>
      <match_length>255-*</match_length>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="nchar" output_data_type="char(%precision)">
    <match_parameters>
      <match_length>*-254</match_length>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="nchar" output_data_type="char(254)">
    <match_parameters>
      <match_length>255-*</match_length>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="varchar" output_data_type="clob">
    <match_parameters>
      <match_length>0</match_length>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="text" output_data_type="clob"/>

  <data_type_mapping match_data_type="ntext" output_data_type="clob"/>

  <data_type_mapping match_data_type="nvarchar" output_data_type="clob">
    <match_parameters>
      <match_length>0</match_length>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="nvarchar" output_data_type="varchar(%precision)"/>

  <data_type_mapping match_data_type="date" output_data_type="timestamp"/>

  <data_type_mapping match_data_type="datetime" output_data_type="timestamp"/>

  <data_type_mapping match_data_type="datetime2" output_data_type="timestamp"/>

  <data_type_mapping match_data_type="smalldatetime" output_data_type="timestamp"/>

  <data_type_mapping match_data_type="time" output_data_type="time"/>

  <data_type_mapping match_data_type="datetimeoffset" output_data_type="timestamp"/>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp"/>

  <data_type_mapping match_data_type="image" output_data_type="varchar(8000)"/>

  <data_type_mapping match_data_type="uniqueidentifier" output_data_type="char(36)"/>

</data_type_mapping_set>
