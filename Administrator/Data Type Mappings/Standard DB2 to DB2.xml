<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<data_type_mapping_set name="Standard DB2 to DB2" xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd">

  <mapping_set_description>Standard DB2 to DB2 data type mapping.
Please do not edit this standard mapping file as any changes will be overwritten on upgrades.
See the user guide for information on how to create custom mapping files.</mapping_set_description>

  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_precision>1-4</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal">
    <match_parameters>
      <match_precision>5</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision)">
    <match_parameters>
      <match_precision>6-*</match_precision>
      <match_scale>0</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="decimal" output_data_type="decimal(%precision,%scale)">
    <match_parameters>
      <match_scale>1-*</match_scale>
    </match_parameters>
  </data_type_mapping>

  <data_type_mapping match_data_type="smallint" output_data_type="smallint"/>

  <data_type_mapping match_data_type="integer" output_data_type="integer"/>

  <data_type_mapping match_data_type="bigint" output_data_type="bigint"/>

  <data_type_mapping match_data_type="decfloat" output_data_type="decfloat"/>

  <data_type_mapping match_data_type="double" output_data_type="double"/>

  <data_type_mapping match_data_type="real" output_data_type="real"/>

  <data_type_mapping match_data_type="long varchar" output_data_type="long varchar"/>

  <data_type_mapping match_data_type="clob" output_data_type="clob"/>

  <data_type_mapping match_data_type="date" output_data_type="date"/>

  <data_type_mapping match_data_type="time" output_data_type="time"/>

  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp"/>

</data_type_mapping_set>
