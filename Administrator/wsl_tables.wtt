_TABLE_ dss_batch
create multiset table _DATABASE_dss_batch (
dss_batch_number 	INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1),
dss_batch_load_table	varchar(64) CHARACTER SET LATIN,
dss_batch_load_status	varchar(1) CHARACTER SET LATIN,
dss_batch_load_rows	integer,
dss_batch_src_type	varchar(64) CHARACTER SET LATIN,
dss_batch_src_system	varchar(256) CHARACTER SET LATIN,
dss_batch_src_object	varchar(4000) CHARACTER SET LATIN,
dss_batch_comments	varchar(4000) CHARACTER SET LATIN,
dss_batch_load_date	timestamp,
dss_batch_process_date	timestamp,
dss_batch_process_status varchar(1) CHARACTER SET LATIN
) UNIQUE PRIMARY INDEX (dss_batch_number);

_TABLE_ dss_parameter
create multiset table _DATABASE_dss_parameter (
dss_parameter_name 	varchar(256) CHARACTER SET LATIN NOT NULL,
dss_parameter_value	varchar(4000) CHARACTER SET LATIN,
dss_parameter_comments	varchar(256) CHARACTER SET LATIN,
dss_parameter_type	varchar(1) CHARACTER SET LATIN
) UNIQUE PRIMARY INDEX (dss_parameter_name);


_TABLE_ dss_fact_table
create multiset table _DATABASE_dss_fact_table (
dss_fact_table_key	INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1),
dss_fact_table_name	varchar(64) CHARACTER SET LATIN,
dss_fact_table_type	varchar(24) CHARACTER SET LATIN
) UNIQUE PRIMARY INDEX  (dss_fact_table_key);


_TABLE_ dss_source_system
create multiset table _DATABASE_dss_source_system (
dss_source_system_key	INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1),
dss_source_system_name	varchar(64) CHARACTER SET LATIN,
dss_source_active_from	timestamp,
dss_source_active_to	timestamp
) UNIQUE PRIMARY INDEX (dss_source_system_key);


_TABLE_ dss_load_file
CREATE MULTISET TABLE _DATABASE_dss_load_file ,NO FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL
     (
      dss_load_file_key INTEGER TITLE 'DSS Load File Key' GENERATED BY DEFAULT AS IDENTITY
           (START WITH 1 
            INCREMENT BY 1 
            MINVALUE -2147483647 
            MAXVALUE 2147483647 
            NO CYCLE),
      dss_sequence INTEGER NOT NULL,
      dss_load_sequence INTEGER NOT NULL DEFAULT 0 ,
      dss_time_stamp TIMESTAMP(6) NOT NULL DEFAULT CURRENT_TIMESTAMP(6),
      dss_status CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC NOT NULL DEFAULT '?',
      dss_job VARCHAR(64) CHARACTER SET LATIN NOT CASESPECIFIC NOT NULL DEFAULT '?',
      dss_task VARCHAR(64) CHARACTER SET LATIN NOT CASESPECIFIC NOT NULL DEFAULT '?',
      dss_job_key INTEGER NOT NULL DEFAULT 0 ,
      dss_task_key INTEGER NOT NULL DEFAULT 0 ,
      dss_source_system_key CHAR(10) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'DSS Source System Key' NOT NULL,
      dss_load_table_name VARCHAR(1000) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'DSS Load Table Name' NOT NULL,
      dss_file_name VARCHAR(1000) CHARACTER SET LATIN NOT CASESPECIFIC TITLE 'DSS File Name' NOT NULL,
      dss_load_count INTEGER TITLE 'DSS Load Count' NOT NULL DEFAULT 0 ,
      dss_extract_count INTEGER TITLE 'DSS Extract Count' NOT NULL DEFAULT 0 ,
      dss_extract_start_date DATE FORMAT 'yyyy-mm-dd' TITLE 'DSS Extract Start Date',
      dss_extract_start_time TIME(6) FORMAT 'HH:MI:SS' TITLE 'DSS Extract Start Time',
      dss_extract_end_date DATE FORMAT 'yyyy-mm-dd' TITLE 'DSS Extract End Date',
      dss_extract_end_time TIME(6) FORMAT 'HH:MI:SS' TITLE 'DSS Extract End Time',
      dss_current_flag varchar(1) CHARACTER SET LATIN)
UNIQUE PRIMARY INDEX dss_load_file_idx_PR ( dss_load_file_key );

