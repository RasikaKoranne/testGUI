, wd_message
, wd_db_msg_desc 
FROM _DATABASE_ws_wrk_error_log;

_VIEW_ ws_admin_v_sched
CREATE VIEW _DATABASE_ws_admin_v_sched AS
SELECT CAST('Waiting' AS VARCHAR(20)) "Type"
, wjc_name "Job Name"
, CAST((CASE wjc_status 
    WHEN 'H' THEN 'On Hold'
    WHEN 'R' THEN 'Running'
    WHEN 'P' THEN 'Pending'
    WHEN 'W' THEN 'Waiting'
    WHEN 'C' THEN 'Completed'
    WHEN 'B' THEN 'Blocked'
    WHEN 'F' THEN 'Failed'
    WHEN 'G' THEN 'Failed - Aborted'
    WHEN 'E' THEN 'Error Completion'
    ELSE 'Unknown' 
    END) AS VARCHAR(30)) "Status"
, wjc_sequence "Sequence"
, CAST(wjc_start_after AS TIMESTAMP) "Started/Scheduled"
, CAST(CURRENT_TIMESTAMP AS TIMESTAMP) "Completed"
, CAST(0 AS INTEGER) "Hours Elapsed"
, CAST(0 AS INTEGER) "Minutes Elapsed"
, CAST(0 AS INTEGER) "Okay"
, CAST(0 AS INTEGER) "Info"
, CAST(0 AS INTEGER) "Warn"
, CAST(0 AS INTEGER) "Detail"
, CAST(0 AS INTEGER) "Error"
FROM _DATABASE_ws_wrk_job_ctrl
UNION
SELECT 'Running'
, wjr_name
, CASE wjr_status
    WHEN 'H' THEN 'On Hold'
    WHEN 'R' THEN 'Running'
    WHEN 'P' THEN 'Pending'
    WHEN 'W' THEN 'Waiting'
    WHEN 'C' THEN 'Completed'
    WHEN 'B' THEN 'Blocked'
    WHEN 'F' THEN 'Failed'
    WHEN 'G' THEN 'Failed - Aborted'
    WHEN 'E' THEN 'Error Completion'
    ELSE 'Unknown' 
    END
, wjr_sequence
, CAST(wjr_started AS TIMESTAMP)
, CAST(wjr_completed AS TIMESTAMP)
, COALESCE(CAST((CAST(CURRENT_TIMESTAMP AS DATE)
    - CAST(wjr_started AS DATE))
    + (extract(HOUR FROM CURRENT_TIMESTAMP)
    - extract(HOUR FROM wjr_started)) AS INTEGER),0)
, COALESCE(CAST((CAST(CURRENT_TIMESTAMP AS DATE)
    - CAST(wjr_started AS DATE))
    + ((extract(HOUR FROM CURRENT_TIMESTAMP)*60+extract(MINUTE FROM CURRENT_TIMESTAMP))
    - (extract(HOUR FROM wjr_started)*60+extract(MINUTE FROM wjr_started))) AS INTEGER),0)
, CAST(wjr_okay_count AS INTEGER)
, CAST(wjr_info_count AS INTEGER)
, CAST(wjr_warning_count AS INTEGER)
, CAST(wjr_detail_count AS INTEGER)
, CAST(wjr_error_count AS INTEGER)
FROM _DATABASE_ws_wrk_job_run 
UNION
SELECT 'Finished'
, wjl_name
, CASE wjl_status 
    WHEN 'H' THEN 'On Hold'
    WHEN 'R' THEN 'Running'
    WHEN 'P' THEN 'Pending'
    WHEN 'W' THEN 'Waiting'
    WHEN 'C' THEN 'Completed'
    WHEN 'B' THEN 'Blocked'
    WHEN 'F' THEN 'Failed'
    WHEN 'G' THEN 'Failed - Aborted'
    WHEN 'E' THEN 'Error Completion'
    ELSE 'Unknown' 
    END
, wjl_sequence
, wjl_started
, wjl_completed
, wjl_elapsed_hh
, wjl_elapsed_mi
, wjl_okay_count
, wjl_info_count
, wjl_warning_count
, wjl_detail_count
, wjl_error_count
FROM _DATABASE_ws_wrk_job_log;

_VIEW_ ws_admin_v_task
CREATE VIEW _DATABASE_ws_admin_v_task AS
SELECT CAST('Running' AS VARCHAR(256)) "result_msg"
, wtr_name
, CAST((CASE wtr_run_status 
    WHEN 'H' THEN 'On Hold'
    WHEN 'R' THEN 'Running'
    WHEN 'P' THEN 'Pending'
    WHEN 'W' THEN 'Waiting'
    WHEN 'C' THEN 'Completed'
    WHEN 'B' THEN 'Blocked'
    WHEN 'F' THEN 'Failed'
    WHEN 'G' THEN 'Failed - Aborted'
    WHEN 'E' THEN 'Error Completion'
    ELSE 'Unknown' 
    END) AS VARCHAR(30)) "status"
, wtr_sequence
, wtr_started
, wtr_completed
, COALESCE(CAST((CAST(CURRENT_TIMESTAMP AS DATE)
    - CAST(wtr_started AS DATE))
    + (extract(HOUR FROM CURRENT_TIMESTAMP)
    - extract(HOUR FROM wtr_started)) AS INTEGER),0) "elapsed_HH"
, COALESCE(CAST((CAST(CURRENT_TIMESTAMP AS DATE)
    - CAST(wtr_started AS DATE))
    + ((extract(HOUR FROM CURRENT_TIMESTAMP)*60+extract(MINUTE FROM CURRENT_TIMESTAMP))
    - (extract(HOUR FROM wtr_started)*60+extract(MINUTE FROM wtr_started))) AS INTEGER),0) "elapsed_MI"
, COALESCE(wtr_info_count,0) wtr_info_count
, COALESCE(wtr_warning_count,0) wtr_warning_count
, COALESCE(wtr_detail_count,0) wtr_detail_count
, COALESCE(wtr_rec_inserted,0) wtr_rec_inserted
, COALESCE(wtr_rec_updated,0) wtr_rec_updated
, COALESCE(wtr_rec_replaced,0) wtr_rec_replaced
, COALESCE(wtr_rec_deleted,0) wtr_rec_deleted
, COALESCE(wtr_rec_discarded,0) wtr_rec_discarded
, COALESCE(wtr_rec_rejected,0) wtr_rec_rejected
, COALESCE(wtr_rec_errored,0) wtr_rec_errored
FROM _DATABASE_ws_wrk_task_run 
UNION
SELECT wtl_return_msg
, wtl_name
, CASE wtl_run_status 
    WHEN 'H' THEN 'On Hold'
    WHEN 'R' THEN 'Running'
    WHEN 'P' THEN 'Pending'
    WHEN 'W' THEN 'Waiting'
    WHEN 'C' THEN 'Completed'
    WHEN 'B' THEN 'Blocked'
    WHEN 'F' THEN 'Failed'
    WHEN 'G' THEN 'Failed - Aborted'
    WHEN 'E' THEN 'Error Completion'
    ELSE 'Unknown' 
    END
, wtl_sequence
, wtl_started
, wtl_completed
, CAST(wtl_elapsed_hh AS INTEGER)
, CAST(wtl_elapsed_mi AS INTEGER)
, COALESCE(wtl_info_count,0)
, COALESCE(wtl_warning_count,0)
, COALESCE(wtl_detail_count,0)
, COALESCE(wtl_rec_inserted,0)
, COALESCE(wtl_rec_updated,0)
, COALESCE(wtl_rec_replaced,0)
, COALESCE(wtl_rec_deleted,0)
, COALESCE(wtl_rec_discarded,0)
, COALESCE(wtl_rec_rejected,0)
, COALESCE(wtl_rec_errored,0)
FROM _DATABASE_ws_wrk_task_log;

